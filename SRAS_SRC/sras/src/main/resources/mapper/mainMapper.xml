<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper namespace="com.shinhan.sras.mapper.MainMapper">

    <select id="getSrasList" resultType="com.shinhan.sras.model.Sras">
        SELECT
            REVIEW_DATE,
            REVIEW_ID,
            REVIEWER,
            REVIEW_CONTENT,
            REVIEW_STAR,
            DEVICE_INFO,
            APP_VERSION,
            OS_KIND,
            OS_VERSION,
            TAG,
            REVIEW_REPLY,
            ETC_DATA1,
            ETC_DATA2,
            ETC_DATA3
        FROM
        SRAS_LIST
    </select>

</mapper>